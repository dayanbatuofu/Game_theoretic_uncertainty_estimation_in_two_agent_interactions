              "duration": args.sim_duration,
              "n_agents": e.n_agents,
              "inference_type": args.agent_inference,
              "decision_type": args.agent_decision,
              "sim_dt": args.sim_dt,
              "sim_lr": args.sim_lr,
              "sim_nepochs": args.sim_nepochs}
    s = Simulation(**kwargs)
    s.run()

    # add analysis stuff here
    # s.postprocess()




